config_version=5

[application]

config/name="Soulslike Steam Game"
run/main_scene="res://main/main.tscn"
config/features=PackedStringArray("4.3", "Mobile")
config/icon="res://icon.png"

[autoload]

LimboConsole="*res://addons/limbo_console/limbo_console.gd"
SoundManager="*res://addons/sound_manager/sound_manager.gd"
PauseManager="*res://Singletons/Scripts/PauseManager/pause_manager.gd"
Sound="*res://Globals/sound.gd"
SceneManager="*res://addons/scene_manager/SceneManager.tscn"

[display]

window/size/mode=3
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/vsync/vsync_mode=0

[editor_plugins]

enabled=PackedStringArray("res://addons/limbo_console/plugin.cfg", "res://addons/scene_manager/plugin.cfg", "res://addons/sound_manager/plugin.cfg")

[file_customization]

folder_colors={
"res://globals/": "orange",
"res://player/": "teal",
"res://resources/": "red"
}

